{
  "name": "kaeuzchenlager",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "includedScripts": [],
  "projectType": "application",
  "prefix": "kl",
  "sourceRoot": "./src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "security": {
        "autoCsp": true
      },
      "options": {
        "outputPath": "dist/kaeuzchenlager",
        "browser": "src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "./src/favicon.ico",
          "./src/assets"
        ],
        "styles": [
          "src/app/shell/layout/theme.scss",
          "src/styles.scss",
          "node_modules/material-icons/iconfont/material-icons.scss"
        ],
        "budgets": [
          {
            "type": "initial",
            "maximumWarning": "1.2mb",
            "maximumError": "1.3mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "200kb",
            "maximumError": "200kb"
          }
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "kaeuzchenlager:build:production"
        },
        "development": {
          "buildTarget": "kaeuzchenlager:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "kaeuzchenlager:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "./src"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "coverage/kaeuzchenlager"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "kaeuzchenlager:build",
        "port": 4200,
        "staticFilePath": "dist/kaeuzchenlager/browser",
        "spa": true
      }
    }
  }
}